{
  "name": "variable-speed-limit-sign-bot",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "deploy:dev": "tsc && node ./scripts/deploy.dev.js",
    "deploy:prod": "tsc && node ./scripts/deploy.prod.js",
    "undeploy:dev": "node ./scripts/undeploy.dev.js",
    "undeploy:prod": "node ./scripts/undeploy.prod.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "prisma": "^4.0.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@prisma/client": "^4.0.0",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.0.1",
    "dotenv": "^16.0.1"
  }
}
